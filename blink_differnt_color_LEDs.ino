void setup() {
  pinMode(4,OUTPUT);
  pinMode(8,OUTPUT);
  pinMode(12,OUTPUT);
}

void loop() {
  digitalWrite(4,HIGH);
  delay(1000);
  digitalWrite(4,HIGH);
  delay(1000);
  digitalWrite(8,HIGH);
  delay(1000);
  digitalWrite(8,HIGH);
  delay(1000);
  digitalWrite(12,HIGH);
  delay(1000);
  digitalWrite(12,HIGH);
  delay(1000);
}
